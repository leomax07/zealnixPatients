{
  "name": "garuda_patients",
  "version": "0.0.1",
  "private": true,
  "author": "Arulraj V",
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "prettify": "npx prettier --write ./src/**/*.{ts,tsx}",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "commit": "cz",
    "check:types": "tsc --noEmit",
    "prfiles:lint": "node ./prlint.js",
    "precommit": "yarn prfiles:lint && tsc --noEmit && lint-staged && yarn lint",
    "ios": "ENVFILE=.env.development react-native run-ios",
    "ios:prod": "ENVFILE=.env.production react-native run-ios",
    "android": "SET ENVFILE=.env.development && npx react-native run-android",
    "android:prod": "SET ENVFILE=.env.production react-native run-android",
    "build:debug-dev": "export ENVFILE=.env.development && cd android && ./gradlew assembleDebug && cd ../",
    "build:dev": "export ENVFILE=.env.development && cd android && ./gradlew assembleRelease && cd ..",
    "build:prod": "SET ENVFILE=.env.production && cd android && ./gradlew assembleRelease && cd ..",
    "build:bundle": "export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleRelease && cd ../",
    "clear": "cd android && ./gradlew clean && cd ../",
    "prebuild": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "postversion": "react-native-version --never-amend"
  },
  "dependencies": {
    "@notifee/react-native": "^7.6.1",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/clipboard": "1.5.1",
    "@react-native-community/netinfo": "^9.3.7",
    "@react-native-firebase/app": "^17.4.0",
    "@react-native-firebase/messaging": "^17.4.0",
    "@react-navigation/bottom-tabs": "^6.5.4",
    "@react-navigation/material-top-tabs": "^6.5.3",
    "@react-navigation/native": "^6.1.3",
    "@react-navigation/native-stack": "^6.9.9",
    "@react-navigation/stack": "^6.3.12",
    "@reduxjs/toolkit": "^1.9.2",
    "agora-react-native-rtm": "^1.5.1",
    "agora-rn-uikit": "^5.0.1",
    "axios": "^1.4.0",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "react-delegate-component": "^1.0.0",
    "react-native": "0.71.3",
    "react-native-agora": "^4.1.2",
    "react-native-animatable": "^1.3.3",
    "react-native-calendars": "1.1293.0",
    "react-native-config": "^1.5.0",
    "react-native-date-picker": "^4.2.13",
    "react-native-device-info": "^10.4.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-google-places-autocomplete": "^2.5.1",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-maps": "^1.4.0",
    "react-native-network-logger": "^1.14.1",
    "react-native-pager-view": "^6.1.2",
    "react-native-permissions": "^3.8.0",
    "react-native-razorpay": "^2.3.0",
    "react-native-reanimated": "^2.14.4",
    "react-native-restart": "^0.0.27",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.19.0",
    "react-native-sound": "^0.11.2",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.8.0",
    "react-native-swiper": "^1.6.0",
    "react-native-swiper-flatlist": "3.0.14",
    "react-native-tab-view": "^3.3.4",
    "react-native-version": "^4.0.0",
    "react-native-video": "^6.0.0-alpha.6",
    "react-otp-input": "^3.0.2",
    "react-redux": "^8.0.5",
    "rn-fetch-blob": "^0.12.0",
    "squashapps-react-native-uikit": "^0.0.39"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@commitlint/prompt": "^17.4.2",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "commitizen": "^4.2.6",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-recommended": "^4.1.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.17.0",
    "git-changed-files": "^1.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^13.1.0",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": "18.14.0"
  },
  "resolutions": {
    "@types/react": "18.0.21"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint --ext .js,.jsx,.ts,.tsx"
    ]
  }
}
